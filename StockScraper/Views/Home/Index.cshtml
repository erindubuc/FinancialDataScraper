@{
    ViewBag.Title = "Home Page";
}
@Styles.Render("~/Content/Custom.css")

 
    <div class="jumbotron">

        <h1>StockScraper</h1>
        <p class="lead">Building a stock portfolio has never been <br />
        more within reach!</p>
        <p class="btn btn-primary btn-lg">Learn more &raquo;</p>
    </div>

    <div class="row">
        <div class="col-md-6">
            <h2>@Html.ActionLink("Getting Started", "Index", "https://finance.yahoo.com/")</h2>
            <p>
                Check out Yahoo! Finance and build your personal portfolio of the stocks you want to track.
            </p>
        </div>
        <div class="col-md-6">
            <h2>@Html.ActionLink("Feel Secure", "Index", "Account/Register")</h2>
            <p>All users access personal financial portfolios through our secure membership area.</p>
        </div>
    </div>
        
    